def findTotalOccurences(listOfNumbers,target):

    result=0
    n=len(listOfNumbers)
    for index in range(n):
        if listOfNumbers[index]%2==0:
           result=result+1
    return result

listOfNumbers=[1,2,3,4,5,6,37,7,2,1]
target=35
result=findTotalOccurences(listOfNumbers,target)
print("total number of occurence are:",result)
